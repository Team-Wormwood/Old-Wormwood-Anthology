					{
						"filters": {
							"all_of": [
								{
									"test": "has_mob_effect",
									"subject": "self",
									"value": "bad_omen"
								},
								{
									"test": "is_in_village",
									"subject": "self",
									"value": true
								}
							]
						},
						"event": "minecraft:trigger_raid"
					},
